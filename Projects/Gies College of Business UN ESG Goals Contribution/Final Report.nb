    RowBox[{
     RowBox[{"{", 
      RowBox[{"_", ",", "department_"}], "}"}], "->", "department"}]}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"departmentSums", "=", 
   RowBox[{"Total", "/@", 
    RowBox[{"(", 
     RowBox[{"groupedCounts", "/.", " ", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{"_", ",", "department_"}], "}"}], "->", "department"}]}], 
     ")"}]}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"departmentCounts", "=", 
   RowBox[{"AssociationThread", "[", 
    RowBox[{"departmentList", "->", "departmentSums"}], "]"}]}], 
  ";"}], "\n", 
 RowBox[{
  RowBox[{"departmentCountsList", "=", 
   RowBox[{"KeyValueMap", "[", 
    RowBox[{
     RowBox[{
      RowBox[{"{", 
       RowBox[{
        RowBox[{"#1", "[", 
         RowBox[{"[", "2", "]"}], "]"}], ",", "#2"}], "}"}], "&"}], ",", 
     "groupedCounts"}], "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"departmentCounts", "=", 
   RowBox[{"Association", "[", "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"Do", "[", 
   RowBox[{
    RowBox[{"With", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{
        RowBox[{"dept", "=", 
         RowBox[{"pair", "[", 
          RowBox[{"[", "1", "]"}], "]"}]}], ",", 
        RowBox[{"count", "=", 
         RowBox[{"pair", "[", 
          RowBox[{"[", "2", "]"}], "]"}]}]}], "}"}], ",", 
      RowBox[{
       RowBox[{"departmentCounts", "[", "dept", "]"}], "=", 
       RowBox[{
        RowBox[{"Lookup", "[", 
         RowBox[{"departmentCounts", ",", "dept", ",", "0"}], "]"}], "+", 
        "count"}]}]}], "]"}], ",", 
    RowBox[{"{", 
     RowBox[{"pair", ",", "departmentCountsList"}], "}"}]}], "]"}], 
  ";"}]}], "Input",
