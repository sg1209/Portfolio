  "\[IndentingNewLine]"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"containsDictWordQ", "[", "article_", "]"}], ":=", 
   RowBox[{"AnyTrue", "[", 
    RowBox[{"dictList", ",", 
     RowBox[{
