       RowBox[{"article", ",", "#", ",", 
        RowBox[{"IgnoreCase", "->", "True"}]}], "]"}], "&"}]}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"groupedCounts", "=", 
   RowBox[{"GroupBy", "[", 
    RowBox[{
     RowBox[{"Transpose", "[", 
      RowBox[{"{", 
       RowBox[{"names", ",", "departments", ",", "articles"}], "}"}], "]"}], 
     ",", 
     RowBox[{"Most", "->", "Last"}], ",", 
     RowBox[{
      RowBox[{"Total", "[", 
       RowBox[{"Boole", "[", 
        RowBox[{"Map", "[", 
         RowBox[{"containsDictWordQ", ",", "#"}], "]"}], "]"}], "]"}], 
      "&"}]}], "]"}]}], ";"}], "\n", 
 RowBox[{
  RowBox[{"departmentList", "=", 
   RowBox[{
